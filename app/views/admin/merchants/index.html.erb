<h1><center>Merchant Index</center></h1>
<table>
  <thead>
    <tr>
      <th>Merchant Name</th>

    </tr>
  </thead>
  <tbody>
    <% @merchants.each do |merchant| %>
      <tr>
        <td><%= link_to merchant.name, admin_merchant_path(merchant) %></td>
      </tr>
    <% end %>
  </tbody>
</table>